{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [

    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/admin",
      "methods": [
        "POST",
        "PUT",
        "GET"
      ],
      "dest": "/routes/admin.js"
    }
  ]
}